#include <stdio.h>
#include<conio.h>
int main()
{
    int a[10];
    int i;
    printf("enter 10 numbers");
    for(i=0;i<=9;i++)
    scanf("%d",&a[i]);
    for(i=0;i<=9;i++)
    {
        if(a[0]>a[i])
        {
            a[0]=a[i];
        }
    }
    printf("smallest element is %d",a[0]);
    return 0;
}
