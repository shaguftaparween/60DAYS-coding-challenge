#include <stdio.h>
#include<conio.h>
int main()
{
    int a[5],even_sum=0,odd_sum=0,i;
    printf("enter 5 numbers");
    for(i=0;i<=4;i++)
    scanf("%d",&a[i]);
    for(i=0;i<=4;i++)
    {
    if(a[i]%2==0)
    even_sum=even_sum+a[i];
    else
    odd_sum=odd_sum+a[i];
    }
    printf("sum of even numbers are %d\n",even_sum);
    printf("sum of odd numbers are %d",odd_sum);
    return 0;
}
