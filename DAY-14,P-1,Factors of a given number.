#include <stdio.h>
#include<conio.h>
void factor(int a);
int main()
{
    int a;
    printf("enter a number");
    scanf("%d",&a);
    printf("factors are:\n");
    factor(a);
    getch();
    return 0;
}
void factor(int a)
{
    int i;
    for(i=1;i<=a;i++)
    {
    if(a%i==0)
    printf("%d\n",i);
    }
}

