#include <stdio.h>
#include <conio.h>
int ssquqre(int n);
int main()
{
    int n,s;
    printf("enter a number");
    scanf("%d",&n);
    s=ssquqre(n);
    printf("sum of square of natural no is %d",s);
    getch();
    return 0;
}
int ssquqre(int n)
{
    int i=1,s=0;
    for(i=1;i<=n;i++)
    {
    s=s+i*i;
}
return s;
}
