#include <stdio.h>
#include <conio.h>
void oddsum();
int main()
{
    oddsum();
    getch();
    return 0;
}
void oddsum()
{
    int i=1,n,s=0;
    printf("enter a number");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
    if(i%2==!0)
    s=s+i;
    }
    printf("sum of odd natural no is %d",s);
}
