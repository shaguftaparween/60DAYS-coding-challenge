Q..sum of n natural number.
#include <stdio.h>
#include<conio.h>
int sum(int n);
int main()
{
    int n,s;
    printf("enter a number");
    scanf("%d",&n);
    s=sum(n);
    getch();
    return 0;
}
int sum(int n)
{
    int s=0,i;
    for(i=1;i<=n;i++)
    {
        s=s+i;
    }
    printf("%d",s);
}




